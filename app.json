{
  "expo": {
    "name": "loginApp",
    "slug": "loginApp",
    "owner": "firearcher717",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "loginapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.firearcher717.loginapp",
      "supportsTablet": true
    },
    "android": {
      "package": "com.firearcher717.loginapp",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": { "backgroundColor": "#000000" }
        }
      ]
    ],
    "extra": {
      "eas": { "projectId": "a9068f10-c1f0-498e-b668-f6dd08f2b943" },
      "iosClientId": "803821575234-2c8k11n085spui79bp5nn0h9uljrqovo.apps.googleusercontent.com",
      "androidClientId": "803821575234-imvqlbp6nj82labgvsnfl6fr431pr889.apps.googleusercontent.com",
      "webClientId": "803821575234-c2v1g4ph2cvube13itj48jd5bvin97uf.apps.googleusercontent.com"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
